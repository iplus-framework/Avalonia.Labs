<Project>
  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <LangVersion>preview</LangVersion>
    <AvaloniaVersion>11.3.8</AvaloniaVersion>
    <AvaloniaUseCompiledBindingsByDefault>true</AvaloniaUseCompiledBindingsByDefault>
    <AvaloniaSampleVersion>11.3.8</AvaloniaSampleVersion>
    <SkiaSharpVersion>3.119.0</SkiaSharpVersion>
    <AvaloniaPublicKey></AvaloniaPublicKey>
  </PropertyGroup>

  <PropertyGroup>
    <AvaloniaBuildFramework>net8.0</AvaloniaBuildFramework>
    <AvaloniaRootDir>..\..\..\av_main</AvaloniaRootDir>
    <AvaloniaNativeBuildDir>$(AvaloniaRootDir)\packages\Avalonia\bin</AvaloniaNativeBuildDir>
    <AvaloniaMarkupBuildDir>$(AvaloniaRootDir)\src\Markup\Avalonia.Markup.Xaml.Loader\bin</AvaloniaMarkupBuildDir>
    <AvaloniaSkiaBuildDir>$(AvaloniaRootDir)\src\Skia\Avalonia.Skia\bin</AvaloniaSkiaBuildDir>
    <AvaloniaDataGridIncludeDir>..\..\..\Avalonia.Controls.DataGrid\artifacts\bin</AvaloniaDataGridIncludeDir>
    <UseAvaloniaFork>true</UseAvaloniaFork>
    <IncludeAvaloniaGenerators Condition="'$(UseAvaloniaFork)' == 'True'">true</IncludeAvaloniaGenerators>
  </PropertyGroup>

  <!-- Explicitly define the packages SourceRoot with trailing slash -->
  <!--<ItemGroup>
    <SourceRoot Include="D:\Devel\iPlusGit\V5\packages\" />
  </ItemGroup>-->
</Project>
